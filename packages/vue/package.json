{
  "name": "@meldui/vue",
  "version": "0.1.4",
  "description": "MeldUI Vue component library built on shadcn-vue",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "import": "./dist/styles/index.css",
      "require": "./dist/styles/index.css"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/meldui/meldui.git",
    "directory": "packages/vue"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write ."
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.9",
    "@internationalized/date": "^3.10.0",
    "@tanstack/vue-table": "^8.21.3",
    "@unovis/ts": "^1.6.2",
    "@unovis/vue": "^1.6.2",
    "@vueuse/core": "^14.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3-array": "^3.2.4",
    "d3-color": "^3.1.0",
    "d3-force": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-time": "^3.1.0",
    "d3-transition": "^3.0.1",
    "embla-carousel": "^8.6.0",
    "embla-carousel-vue": "^8.6.0",
    "radix-vue": "^1.9.0",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^2.6.0",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "vue-input-otp": "^0.3.1",
    "vue-sonner": "^2.0.9"
  },
  "peerDependencies": {
    "@meldui/tabler-vue": "workspace:*",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0-alpha.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/tsconfig": "^0.5.0",
    "tailwindcss": "^4.0.0-alpha.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  },
  "keywords": [
    "vue",
    "components",
    "design-system",
    "shadcn",
    "tailwind"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
