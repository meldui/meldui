{
  "name": "meldui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build --filter=!task-manager",
    "dev": "turbo dev",
    "lint": "oxlint .",
    "lint:fix": "oxlint --fix .",
    "format": "oxfmt .",
    "format:check": "oxfmt --check .",
    "check": "pnpm lint && pnpm format:check",
    "check:fix": "pnpm lint:fix && pnpm format",
    "prepare": "husky",
    "storybook:vue": "pnpm --filter vue-storybook storybook",
    "build:storybook:vue": "pnpm --filter vue-storybook build-storybook",
    "dev:example": "pnpm --filter task-manager dev",
    "build:example": "pnpm --filter task-manager build",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "docs:preview": "pnpm --filter docs preview",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "pnpm build && changeset publish",
    "release": "pnpm check:fix && pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "husky": "^9.1.7",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "vite": "8.0.0-beta.10"
  },
  "packageManager": "pnpm@10.13.1"
}
